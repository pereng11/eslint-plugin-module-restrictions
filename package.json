{
  "version": "0.2.1",
  "name": "eslint-plugin-module-restrictions",
  "license": "MIT",
  "author": "pereng11",
  "repository": {
    "type": "git",
    "url": "https://github.com/pereng11/eslint-plugin-module-restrictions.git"
  },
  "keywords": [
    "eslint",
    "plugin",
    "module",
    "architecture",
    "restrict",
    "imports",
    "fsd"
  ],
  "main": "./dist/eslint/index.js",
  "types": "./dist/eslint/index.d.ts",
  "files": [
    "dist/",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/eslint/index.d.ts",
      "require": "./dist/eslint/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "minimatch": "^10.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.2.1",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "typescript": ">=4.0.0"
  }
}
